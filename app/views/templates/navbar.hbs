<div class="navbar-header">
  <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
    <span class="sr-only">Toggle navigation</span>
    {{#each items}}<span class="icon-bar"></span>{{/each}}
  </button>
  <a
    class="{{#ifActive main.title}}active-nav{{/ifActive}} navbar-brand"
    href="{{main.href}}">
    {{main.title}}
  </a>
</div>
<div class="collapse navbar-collapse">
  <ul class="nav navbar-nav">
    {{#each items}}
    <li {{#ifActive title}}class="active"{{/ifActive}}>
      <a href="{{href}}">{{title}}</a>
    </li>
    {{/each}}
  </ul>
</div><!--/.nav-collapse -->
